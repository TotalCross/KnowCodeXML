<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/container" android:layout_width="match_parent"
	android:layout_height="match_parent" android:orientation="vertical"
	tools:context="com.tonybeltramelli.android_gui.MainActivity">
	<FrameLayout 
	android:id="@+id/content"
		android:layout_width="match_parent"
		android:layout_height="match_parent" 
		android:layout_weight="1"
		android:padding="10dp">
		<LinearLayout 
			android:layout_width="match_parent"
			android:layout_height="match_parent" 
			android:orientation="vertical">
			<LinearLayout 
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal" 
				android:paddingTop="10dp"
				android:paddingBottom="10dp" 
				android:weightSum="1">
				<TextView 
					android:id="@+id/WEurUywPXx"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" 
					android:text="Dsvxv"
					android:textAppearance=
					"@style/TextAppearance.AppCompat.Body2" />
				<SeekBar 
					android:id="@+id/KcfeNtmitS"
					style="@style/Widget.AppCompat.SeekBar.Discrete"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" 
					android:layout_weight="0.9"
					android:max="10" 
					android:progress="5" />
				<TextView 
					android:id="@+id/ddfvSYJrHV"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" 
					android:text="Aooes"
					android:textAppearance="@style/TextAppearance.AppCompat.Body2" />
			</LinearLayout>
			<LinearLayout 
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal" 
				android:paddingTop="10dp"
				android:paddingBottom="10dp" 
				android:weightSum="1">
				<Switch android:id="@+id/ABiQpqAKQq"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" 
					android:paddingRight="10dp"
					android:text="Pswxc" />
			</LinearLayout>
			<LinearLayout android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal" android:paddingTop="10dp"
				android:paddingBottom="10dp" android:weightSum="1">
				<RadioButton android:id="@+id/vwDmDROOZu"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="10dp"
					android:text="Jcdua" />
			</LinearLayout>
			<LinearLayout android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal" android:paddingTop="10dp"
				android:paddingBottom="10dp" android:weightSum="1">
				<Switch android:id="@+id/HCxIIYfoXD"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="10dp"
					android:text="Ybpkc" />
			</LinearLayout>
			<LinearLayout android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal" android:paddingTop="10dp"
				android:paddingBottom="10dp" android:weightSum="1">
				<TextView android:id="@+id/jkjevGDHKF"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:text="Gfjgb"
					android:textAppearance="@style/TextAppearance.AppCompat.Body2" />
				<SeekBar android:id="@+id/yCbZcUHtwd"
					style="@style/Widget.AppCompat.SeekBar.Discrete"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:layout_weight="0.9"
					android:max="10" android:progress="5" />
				<TextView android:id="@+id/XZAcMmTmdi"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:text="Cqboc"
					android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

			</LinearLayout>
		</LinearLayout>
	</FrameLayout>
	<LinearLayout android:layout_width="match_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:weightSum="1">
		<Button android:id="@+id/XieDrFwEfM"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:background="#0ffffff"
			android:layout_weight="1"
			android:drawableBottom="@drawable/ic_home_black_24dp" android:text="" />
		<Button android:id="@+id/vGGDTnCgKt"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:background="#0ffffff"
			android:layout_weight="1"
			android:drawableBottom="@drawable/ic_notifications_black_24dp"
			android:text="" />
		<Button android:id="@+id/qPwKVVksse"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:background="#0ffffff"
			android:layout_weight="1"
			android:drawableBottom="@drawable/ic_home_black_24dp" android:text="" />
		<Button android:id="@+id/IBVfPFJcTT"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:background="#0ffffff"
			android:layout_weight="1"
			android:drawableBottom="@drawable/ic_home_black_24dp" android:text="" />
	</LinearLayout>
</LinearLayout>
